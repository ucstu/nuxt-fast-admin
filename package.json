{
  "name": "@ucstu/nuxt-fast-admin-root",
  "version": "1.1.8",
  "private": true,
  "scripts": {
    "dev": "pnpm run --filter @ucstu/nuxt-fast-admin dev",
    "dev:all": "pnpm run --filter @ucstu/nuxt-fast-* dev",
    "dev:auth": "pnpm run --filter @ucstu/nuxt-fast-auth dev",
    "dev:crud": "pnpm run --filter @ucstu/nuxt-fast-crud dev",
    "dev:nav": "pnpm run --filter @ucstu/nuxt-fast-nav dev",
    "prepare": "pnpm run --no-bail --filter @ucstu/nuxt-* dev:prepare",
    "prepack": "pnpm run --no-bail --filter @ucstu/nuxt-* prepack",
    "release": "pnpm run --no-bail --filter @ucstu/nuxt-* release",
    "lint": "pnpm run --no-bail --filter @ucstu/nuxt-* lint",
    "test": "pnpm run --no-bail --filter @ucstu/nuxt-* test",
    "type-check": "pnpm --no-bail --filter @ucstu/nuxt-* exec tsc --noEmit",
    "clean": "npx --yes shx rm -rf ./**/dist ./**/.nuxt ./**/node_modules"
  },
  "keywords": [
    "nuxt",
    "fast",
    "admin"
  ],
  "author": "UCSTU",
  "license": "MIT"
}
