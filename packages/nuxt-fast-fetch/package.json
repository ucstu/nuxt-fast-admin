{
  "name": "@ucstu/nuxt-fast-fetch",
  "version": "1.1.8",
  "description": "Nuxt fast fetch module",
  "homepage": "https://github.com/ucstu/nuxt-fast-admin/tree/master/packages/nuxt-fast-fetch#readme",
  "repository": "https://github.com/ucstu/nuxt-fast-admin/tree/master/packages/nuxt-fast-fetch",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./types": {
      "types": "./dist/runtime/types/index.d.ts",
      "import": "./dist/runtime/types/index.js"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build && pnpm run client:build",
    "dev": "pnpm run play:dev",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare client",
    "client:dev": "nuxi dev client --port 3300",
    "client:build": "nuxi generate client",
    "play:dev": "nuxi dev playground",
    "play:build": "nuxi build playground",
    "play:prod": "pnpm run prepack && nuxi dev playground",
    "release": "pnpm run lint && pnpm run test && pnpm run prepack && pnpm publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.1.8",
    "@hey-api/openapi-ts": "^0.48.2",
    "@nuxt/devtools-kit": "^1.3.9",
    "@nuxt/devtools-ui-kit": "^1.3.9",
    "@nuxt/kit": "^3.12.3",
    "@ucstu/nuxt-fast-utils": "workspace:^",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.3",
    "@nuxt/test-utils": "^3.13.1",
    "@types/node": "^20.14.10",
    "changelogen": "^0.5.5",
    "eslint": "^9.7.0",
    "nuxt": "^3.12.3",
    "vitest": "^2.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
